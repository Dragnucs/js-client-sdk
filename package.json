{
  "name": "js-client-sdk-repo-manager",
  "version": "1.0.0",
  "description": "Paper.xyz js client for SDK.",
  "files": [
    "dist/**/*.ts"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "patch": "cd ./dist && npm version patch && cd ../",
    "minor": "cd ./dist && npm version minor && cd ../",
    "major": "cd ./dist && npm version major && cd ../",
    "release": "cd ./dist && npm publish && cd ../"
  },
  "keywords": [],
  "author": "Paper.xyz <team@paper.xyz>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/paperxyz/js-client-sdk/issues"
  },
  "homepage": "https://github.com/paperxyz/js-client-sdk",
  "devDependencies": {
    "@types/node": "^18.7.11",
    "tsup": "^6.2.2",
    "typescript": "^4.7.4"
  }
}
